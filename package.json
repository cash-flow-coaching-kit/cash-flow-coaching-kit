{
	"name": "cash-flow-coaching-kit",
	"version": "0.0.1",
	"author": "Cash Flow Coaching Kit",
	"description": "Cash Flow Coaching Kit",
	"license": "UNLICENSED",
	"homepage": "./",
	"main": "public/electron.js",
	"dependencies": {
		"@date-io/date-fns": "1.x",
		"@imjs/electron-differential-updater": "^5.1.4",
		"@loadable/component": "5.14.1",
		"@material-ui/core": "4.11.2",
		"@material-ui/icons": "^4.11.2",
		"@material-ui/lab": "^4.0.0-alpha.53",
		"@material-ui/pickers": "^3.2.10",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"@types/body-scroll-lock": "^2.6.1",
		"@types/detect-browser": "^4.0.0",
		"@types/file-saver": "^2.0.1",
		"@types/jest": "^24.0.0",
		"@types/jszip": "^3.4.1",
		"@types/loadable__component": "5.13.1",
		"@types/lodash-es": "4.17.4",
		"@types/node": "^12.0.0",
		"@types/pdfmake": "0.1.16",
		"@types/qs": "6.9.5",
		"@types/react": "^16.9.0",
		"@types/react-beautiful-dnd": "^12.1.2",
		"@types/react-dom": "^16.9.0",
		"@types/react-router-dom": "5.1.6",
		"@types/reactour": "^1.17.0",
		"@types/uuid": "8.3.0",
		"@xstate/react": "^0.8.1",
		"body-scroll-lock": "3.1.5",
		"concurrently": "^6.0.2",
		"core-js": "3.8.1",
		"date-fns": "2.16.1",
		"detect-browser": "5.2.0",
		"dexie": "3.0.3",
		"dexie-export-import": "^1.0.0-rc.2",
		"electron-is-dev": "^2.0.0",
		"electron-notarize": "^1.0.0",
		"electron-updater": "^4.3.8",
		"events": "^3.3.0",
		"exceljs": "4.2.0",
		"file-saver": "2.0.5",
		"formik": "2.2.6",
		"jszip": "^3.5.0",
		"lodash-es": "^4.17.15",
		"node-sass": "^4.14.0",
		"p-min-delay": "^3.1.0",
		"pdfmake": "^0.1.65",
		"qs": "^6.9.4",
		"react": "^16.13.1",
		"react-app-polyfill": "^1.0.6",
		"react-beautiful-dnd": "^13.0.0",
		"react-dom": "^16.13.1",
		"react-ga": "^3.3.0",
		"react-router-dom": "^5.1.2",
		"react-scripts": "^3.4.1",
		"reactour": "^1.17.0",
		"regenerator-runtime": "^0.13.5",
		"source-map-explorer": "2.5.1",
		"styled-components": "^4.0.0",
		"typescript": "~3.7.2",
		"uuid": "8.3.2",
		"wait-on": "^5.3.0",
		"whatwg-fetch": "3.5.0",
		"xlsx": "^0.17.0",
		"xstate": "4.15.1",
		"yarn": "1.22.10",
		"youtube-dl": "^3.5.0"
	},
	"scripts": {
		"start:web": "react-scripts start",
		"start:ie11": "yarn build && serve build",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"test:unit": "react-scripts test --env=jsdom",
		"test:coverage": "react-scripts test --env=jsdom --watchAll=false --coverage",
		"eject": "react-scripts eject",
		"analyze": "source-map-explorer 'build/static/js/*.js'",
		"clean": "rimraf build",
		"start": "run-p build start:web",
		"electron:serve": "export NODE_ENV=dev && concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
		"electron:package": "electron-builder -c.extraMetadata.main=build/electron.js -mw --publish always",
		"preelectron:package": "run-s refresh:offlinedoc build",
		"refresh:offlinevid": "node ./public/start-refresh-youtube-videos.mjs",
		"refresh:offlinedoc": "node ./public/start-refresh-website-statics.mjs"
	},
	"build": {
		"productName": "Cash Flow Coaching Kit",
		"appId": "com.dcfck.app",
		"afterSign": "assets/mac/notarize.js",
		"directories": {
			"buildResources": "build"
		},
		"dmg": {
			"sign": false
		},
		"mac": {
			"icon": "build/icon.icns",
			"hardenedRuntime": true,
			"gatekeeperAssess": false,
			"entitlements": "assets/mac/entitlements.mac.plist",
			"entitlementsInherit": "assets/mac/entitlements.mac.plist",
			"category": "public.app-category.finance",
			"type": "distribution"
		},
		"win": {
			"icon": "build/icon.ico",
			"publisherName": "Cash Flow Coaching Kit",
			"signAndEditExecutable": true,
			"verifyUpdateCodeSignature": false,
			"target": "nsis"
		},
		"asarUnpack": [
			"**/*.node"
		],
		"npmRebuild": false,
		"publish": [
			{
				"provider": "github",
				"owner": "cash-flow-coaching-kit",
				"repo": "cash-flow-coaching-kit",
				"publishAutoUpdate": true,
				"releaseType": "prerelease"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cash-flow-coaching-kit/cash-flow-coaching-kit.git"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"IE 11"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^9.1.1",
		"@commitlint/config-conventional": "^9.1.1",
		"@typescript-eslint/eslint-plugin": "^2.29.0",
		"commitizen": "4.2.2",
		"cz-conventional-changelog": "3.3.0",
		"electron": "^13.1.2",
		"electron-builder": "^22.10.5",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-typescript": "^7.2.1",
		"eslint-config-prettier": "^6.11.0",
		"eslint-import-resolver-typescript": "2.3.0",
		"eslint-plugin-fp": "^2.3.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jsx-a11y": "6.4.1",
		"eslint-plugin-prettier": "3.3.0",
		"eslint-plugin-react": "7.21.5",
		"fake-indexeddb": "^3.0.0",
		"foreman": "^3.0.1",
		"husky": "4.3.6",
		"npm-run-all": "^4.1.5",
		"prettier": "2.2.1",
		"pretty-quick": "^2.0.1",
		"puppeteer": "^9.0.0",
		"typescript-eslint-parser": "^22.0.0"
	},
	"eslintIgnore": [
		"**/*.d.ts",
		"src/serviceWorker.ts",
		"public/**/*",
		"build/**/*",
		"commitlint.config.js",
		"src/start-react.js",
		"src/start.js",
		"**/*.test.ts",
		"src/setupTests.ts"
	],
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,ts}",
			"!**/node_modules/**",
			"!src/serviceWorker.ts",
			"!src/start-react.js",
			"!src/start.js",
			"!src/**/style.ts",
			"!src/**/styles.ts",
			"!src/**/machine.ts"
		],
		"extraGlobals": [
			"window"
		],
		"transformIgnorePatterns": [
			"<rootDir>/node_modules/(?!lodash-es)"
		]
	}
}